```elixir
defmodule Helper do
  def duplicate_event(evid) do
    artweek_tag_uuid = MavuTags.resolve_uuid!("artweek_copy")

    event = Event.resolve!(evid)

    params =
      event
      |> strip_non_attributes(skip_id: true)
      |> Map.put(:memo, "copy of #{evid}")
      |> Map.put(:tags, [artweek_tag_uuid])
      |> Map.put(:legacy_event_id, nil)

    Event
    |> Ash.Changeset.for_create(:create, params)
    |> Ash.create()
    |> case do
      {:ok, newev} ->
        duplicate_showtimes(event.id, newev.id)

        newev

      {:error, error_info} ->
        {:error, {:foo, "error on foo", error_info}}
    end
  end

  def duplicate_showtimes(old_ev_id, new_ev_id) do
    artweek_tag_uuid = MavuTags.resolve_uuid!("artweek_copy")

    showtimes =
      Showtime
      |> Ash.Query.filter(event_id: old_ev_id)
      |> Ash.Query.filter(day1 >= "2023-11-10" and day1 <= "2023-11-17")
      |> Ash.read!()

    for st <- showtimes do
      st |> MavuUtils.log("#clcyan   mwuits-debug 2024-07-09_12:57  ", :info)

      params =
        st
        |> strip_non_attributes(skip_id: true)
        |> Map.put(:day1, Date.shift(st.day1, year: 1))
        |> Map.put(:day2, Date.shift(st.day2, year: 1))
        |> Map.put(:tags, [artweek_tag_uuid])
        |> Map.put(:event_id, new_ev_id)
        |> Map.put(:legacy_id, nil)
        |> MavuUtils.log("#clcyan   mwuits-debug 2024-07-09_12:57  ", :info)

      Showtime
      |> Ash.Changeset.for_create(:create, params)
      |> Ash.create!()
    end
  end

  def strip_non_attributes(rec, opts) when is_struct(rec) do
    rec = rec |> Map.take(attribute_names(rec.__struct__))

    if opts[:skip_id] do
      rec |> Map.delete(:id)
    else
      rec
    end
  end

  def strip_non_attributes(a, _opts), do: a

  def attribute_names(resource) when is_atom(resource) do
    resource |> Ash.Resource.Info.attributes() |> Enum.map(& &1.name)
  end
end
