```elixir
for %{new: new_id, old: old_id} <- idmap do
  Event.resolve(old_id)
  |> case do
    {:ok, rec} ->
      rec
      |> Map.get(:image)
      |> case do
        nil ->
          nil

        mf_id ->
          Event.resolve!(new_id)
          |> Ash.Changeset.for_update(:update, %{image: mf_id})
          |> Ash.update!()
      end

    {:error, error_info} ->
      {:error, {:foo, "error on foo", error_info}}
  end

  {new_id, old_id}
end
